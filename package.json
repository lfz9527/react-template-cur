{
    "name": "react-template",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "keywords": [
        "React",
        "JSX",
        "TSX",
        "Webpack",
        "React Router",
        "ESLint",
        "template"
    ],
    "scripts": {
        "start": "npm run dev",
        "build": "cross-env NODE_ENV=production node build/scripts/build.js",
        "build:measure": "cross-env NODE_ENV=measure node build/scripts/build.js",
        "dev": "cross-env NODE_ENV=development node build/scripts/dev.js",
        "clear": "rimraf package-lock.json && rimraf yarn.lock && rimraf node_modules && yarn install",
        "stats": "cross-env NODE_ENV=production node build/scripts/build.js --stats",
        "analyze": "cross-env NODE_ENV=production node build/scripts/build.js --analyze",
        "prepare": "husky install",
        "lint": "npm run lint-eslint && npm run lint-stylelint",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src --fix",
        "lint-stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss} --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js}": [
            "eslint --config .eslintrc.js"
        ],
        "*.{css,less,scss}": [
            "stylelint --config .stylelintrc.js"
        ],
        "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-runtime": "^7.25.4",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-react": "^7.24.7",
        "@babel/preset-typescript": "^7.25.7",
        "@babel/runtime-corejs3": "^7.25.7",
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@eslint/js": "^9.11.1",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/webpack-env": "^1.18.5",
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.60.1",
        "babel-loader": "^9.2.1",
        "bfj": "^9.0.2",
        "chalk": "4.0.0",
        "compression-webpack-plugin": "^11.1.0",
        "copy-webpack-plugin": "^12.0.2",
        "core-js": "^3.38.1",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "detect-port-alt": "^1.1.6",
        "dotenv": "^16.4.5",
        "dotenv-expand": "^11.0.6",
        "eslint": "8.43.0",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "filesize": "9.0.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "globals": "^15.10.0",
        "gzip-size": "6",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^9.1.6",
        "less": "^4.2.0",
        "less-loader": "^12.2.0",
        "lint-staged": "^15.2.10",
        "mini-css-extract-plugin": "^2.9.1",
        "postcss": "^8.4.47",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^8.1.1",
        "postcss-preset-env": "^10.0.6",
        "prettier": "^3.3.3",
        "recursive-readdir": "^2.2.3",
        "rimraf": "5.0.0",
        "strip-ansi": "^6.0.0",
        "style-loader": "^4.0.0",
        "stylelint": "14.16.1",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "29.0.0",
        "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
        "stylelint-order": "^6.0.4",
        "svg-sprite-loader": "^6.0.11",
        "terser-webpack-plugin": "^5.3.10",
        "typescript": "^5.0.3",
        "webpack": "^5.95.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "^6.0.1",
        "webpackbar": "^6.0.1"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "engines": {
        "node": ">= 16.0.0",
        "npm": ">= 7.0.0",
        "yarn": ">= 1.22.17"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "ie >= 9",
        "not op_mini all"
    ]
}
